{
	"manifest_version": 3,
	"name":             "Jira Quick Launcher",
	"version":          "0.0.0",
	"description":      "Jira Quick Launcher",
	"author":           "Brett Harris, Steve Hogdahl",
	"action":   {
		"default_title": "Jira Quick Launcher",
		"default_icon":  "jira.png",
		"default_popup": "index.html"
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Ctrl+J",
				"mac": "Command+J"
			}
		}
	},
	"permissions":      [
		"storage"
	],
	"icons":            {
		"16":  "jira.png",
		"48":  "jira.png",
		"128": "jira.png"
	}
}